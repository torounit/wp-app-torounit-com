{
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "bs": "browser-sync start --server ./www --files 'www/**/*.*' --no-notify",
    "js:build": "rollup -c",
    "js:watch": "rollup -c -w",
    "css:build": "postcss -c postcss.config.json",
    "css:watch": "postcss -c postcss.config.json -w",
    "html:build": "pug src/*.pug -o www",
    "html:watch": "pug src/*.pug -o www -w",
    "image:build": "imagemin src/images/* --out-dir=www/images",
    "build": "npm run js:build & npm run css:build & npm run image:build & npm run html:build",
    "watch": "npm run js:watch & npm run css:watch & npm run html:watch",
    "start": "npm run build && npm run bs & npm run watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "jquery": "^3.1.0",
    "normalize.css": "^4.2.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.11.1",
    "browser-sync": "^2.13.0",
    "imagemin-cli": "^3.0.0",
    "mustache": "^2.2.1",
    "postcss-cli": "^2.5.2",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "postcss-url": "^5.1.2",
    "pug": "^2.0.0-beta4",
    "pug-cli": "^1.0.0-alpha6",
    "rollup": "^0.34.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^3.3.0",
    "rollup-plugin-node-resolve": "^1.7.1",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-watch": "^2.5.0",
    "watch": "^0.19.1"
  }
}
